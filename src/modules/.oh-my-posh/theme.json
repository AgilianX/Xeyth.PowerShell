{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "upgrade",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "foreground": "#5c6b7a",
          "background": "#e3eaf2",
          "properties": {
            "cache_duration": "168h"
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#b3c7d6",
          "foreground": "#2a3a4d",
          "properties": {
            "fetch_version": true
          },
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 "
        },
        {
          "type": "go",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#c7dbe6",
          "foreground": "#2a3a4d",
          "properties": {
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 "
        },
        {
          "type": "julia",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#b3c7d6",
          "foreground": "#2a3a4d",
          "properties": {
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue624 "
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#dbeaf2",
          "foreground": "#2a3a4d",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 "
        },
        {
          "type": "ruby",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#b3c7d6",
          "foreground": "#2a3a4d",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 "
        },
        {
          "type": "azfunc",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "#dbeaf2",
          "foreground": "#2a3a4d",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7"
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "background": "#e3eaf2",
          "foreground": "#2a3a4d",
          "trailing_diamond": "\ue0bc",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
        },
        {
          "type": "root",
          "background": "#e3eaf2",
          "foreground": "#2a3a4d",
          "trailing_diamond": "\ue0bc",
          "style": "diamond",
          "template": " \uf0ad "
        },
        {
          "background": "#b3c7d6",
          "foreground": "#2a3a4d",
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " {{ .FormattedMs }} \uf252 ",
          "type": "executiontime"
        },
        {
          "background": "#1ce726",
          "background_templates": [
            "{{ if gt .Code 0 }}#e71c1c{{ end }}"
          ],
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ffffff{{ else }}#2a3a4d{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status"
        },
        {
          "type": "path",
          "style": "diamond",
          "trailing_diamond": "\ue0bc",
          "background_templates": [
            "{{ if .Writable }}#b3c7d6{{ else }}#e3eaf2{{ end }}"
          ],
          "foreground_templates": [
            "{{ if .Writable }}#2a3a4d{{ else }}#b71c1c{{ end }}"
          ],
          "properties": {
            "home_icon": "~",
            "style": "powerlevel",
            "max_width": 70
          },
          "template": " \uf07c {{ if .Writable }}{{ else }}üîí {{ end }}{{ .Path }} "
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "left",
      "segments": [
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\ue0bc",
          "background": "#2a3a4d",
          "template": " {{.UpstreamIcon }}  {{ .RepoName }} ",
          "properties": {
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "az",
          "style": "diamond",
          "trailing_diamond": "\ue0bc",
          "background": "#547494",
          "template": " ÓØò {{ .TenantDisplayName}} | {{ .Name }} | {{ .EnvironmentName }} "
        },
        {
          "type": "terraform",
          "style": "diamond",
          "trailing_diamond": "\ue0bc",
          "background": "#7fa1c4",
          "template": "{{ .WorkspaceName }}{{ if .Version }} {{ .Version }}{{ end }} ",
          "properties": {
            "fetch_version": true
          }
        },
        {
          "type": "git",
          "foreground": "#0D1117",
          "background": "#689e03",
          "background_templates": [
            "{{ if .Staging.Changed}}#2d6788{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#881717{{ end }}",
            "{{ if gt .Ahead 0 }}#148f7e{{ end }}",
            "{{ if gt .Behind 0 }}#802a5f{{ end }}"
          ],
          "foreground_templates": [
            "{{ if gt .StashCount 0}}#00ffdd{{ end }}",
            "{{ if .Working.Changed}}#ffe600{{ end }}"
          ],
          "style": "powerline",
          "powerline_symbol": "\ue0bc",
          "properties": {
            "branch_icon": " \uf126 ",
            "branch_identical_icon": "‚úÖ",
            "branch_ahead_icon": "üî•",
            "branch_behind_icon": "‚ö†",
            "branch_gone_icon": "üÜï",
            "mapped_branches": {
              "release/*": "üí≤ ",
              "feature/*": "üöÄ ",
              "bugfix/*": "üêõ ",
              "hotfix/*": "üí• "
            },
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} | \uf044 {{ .Working.String }}{{ end }}{{ if .Staging.Changed }} | \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} | \ueb4b {{ .StashCount }}{{ end }} "
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\ue0bc",
          "foreground": "#2a3a4d",
          "background": "#d6d1b3",
          "template": "{{ if .LatestTag }} üè∑Ô∏è  {{ .LatestTag }} {{ end }}"
        }
      ]
    },
    {
      "type": "prompt",
      "newline": true,
      "alignment": "right",
      "segments": [
        {
          "background": "#2a3a4d",
          "foreground": "#0df1d3",
          "leading_diamond": "\ue0ba",
          "style": "diamond",
          "template": "  {{ .UserName }}<transparent> / </>{{ .HostName }} ",
          "type": "session"
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "background": "#15192c",
          "foreground": "#0df1d3",
          "template": " .net {{ .Full }} "
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "project",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "foreground": "#2a3a4d",
          "background": "#6db39a",
          "template": " {{ .Version }} "
        },
        {
          "type": "project",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "foreground": "#cdf19f",
          "background": "#6d74b3",
          "template": " {{ .Target }} "
        },
        {
          "type": "project",
          "style": "diamond",
          "leading_diamond": "\ue0ba",
          "foreground": "#bad6f7",
          "background": "#2a3a4d",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}\uE70C {{ .Name }}{{ end }} "
        }
      ]
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "final_space": true,
  "version": 3,
  "upgrade": {
    "source": "cdn",
    "auto": false,
    "notice": true,
    "interval": "168h"
  }
}
